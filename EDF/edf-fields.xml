<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="edf-fields.xsl"?>
<!-- Any documentation should use MD markup if any kind of content beyond plain text is required. -->
<EDF>
    <field id="time">
        <name>Timestamp</name>
        <documentation>[Internet Timestamps (RFC 3339)](https://tools.ietf.org/html/rfc3339)</documentation>
    </field>
    <field id="lat">
        <name>Latitude</name>
        <documentation>[WGS84](http://en.wikipedia.org/wiki/World_Geodetic_System#A_new_World_Geodetic_System:_WGS_84) in decimal notation</documentation>
    </field>
    <field id="lon">
        <name>Longitude</name>
        <documentation>[WGS84](http://en.wikipedia.org/wiki/World_Geodetic_System#A_new_World_Geodetic_System:_WGS_84) in decimal notation</documentation>
    </field>
    <field id="alt">
        <name>Altitude</name>
        <documentation>altitude above sea level in meters</documentation>
    </field>
    <field id="HR">
        <name>Heartrate</name>
        <documentation>measured in beats per minute</documentation>
    </field>
    <field id="atemp">
        <name>Air Temperature</name>
        <documentation>measured in Celsius</documentation>
    </field>
    <field id="stemp">
        <name>Skin Temperature</name>
        <documentation>measured in Celsius</documentation>
    </field>
    <field id="steps">
        <name>Steps</name>
        <documentation>measured in steps taken during a given time period</documentation>
    </field>
    <field id="GSR">
        <name>Galvanic Skin Response</name>
        <documentation>measured in microsiemens</documentation>
    </field>
</EDF>